{{!--<span class="el-radio__input {{if isChecked ' is-checked '}} {{if disabled ' is-disabled '}}">--}}
{{!--<span class="el-radio__inner"></span>--}}
{{el-radio-input
  value=label
  model=model
  name=name
  item=item
  disabled=disabled
  changed=(action "changed")
  class="el-radio-button__orig-radio"
}}
{{!--<input
  class="el-radio__original"
  :value="label"
  type="radio"
  aria-hidden="true"
  v-model="model"
  @focus="focus = true"
  @blur="focus = false"
  @change="handleChange"
  :name="name"
  :disabled="isDisabled"
  tabindex="-1"
>--}}
{{!--</span>--}}
{{!--<span class="el-radio__label" @keydown.stop>--}}
<span class="el-radio-button__inner">
  {{yield}}
  {{!--<template v-if="!$slots.default">{{label}}</template>--}}
</span>

