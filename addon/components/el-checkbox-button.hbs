
  {{input
    type="checkbox"
    checked=value
    name=name
    disabled=isDisabled
    tabindex=tabindex
    autofocus=autofocus
    form=form
    class="el-checkbox-button__original"
    change=(action "handleChange")
  }}
  {{! template-lint-disable no-inline-styles }}
<span class="el-checkbox-button__inner" style={{this.style}}>
  {{#if hasBlock}}
    {{yield}}
  {{else}}
    {{label}}
  {{/if}}
</span>

